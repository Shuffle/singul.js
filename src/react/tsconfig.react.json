{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "jsx": "react",
    "lib": ["DOM", "ES2022"],
    "declaration": true,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "rootDir": ".",
    "outDir": "../../dist/react",
    "allowJs": false,
    "strict": true,
    "moduleResolution": "Node"
  },
  "include": ["index.tsx"]
}

